{
    "name": "nfse-nacional/nfse-php",
    "description": "This is my package nfse",
    "keywords": [
        "Nfse",
        "nfse"
    ],
    "homepage": "https://github.com/nfse/nfse",
    "license": "MIT",
    "authors": [
        {
            "name": "a21ns1g4ts",
            "email": "a21ns1g4ts@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^11.47",
        "spatie/laravel-data": "^4.8"
    },
    "require-dev": {
        "illuminate/translation": "^11.47",
        "illuminate/validation": "^11.47",
        "laravel/pint": "^1.0",
        "pestphp/pest": "^4.0",
        "spatie/ray": "^1.28",
        "spatie/typescript-transformer": "*"
    },
    "autoload": {
        "psr-4": {
            "Nfse\\Nfse\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nfse\\Nfse\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
